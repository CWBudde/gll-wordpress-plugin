{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "gll-info/gll-info",
	"version": "0.1.0",
	"title": "GLL File Viewer",
	"category": "media",
	"icon": "format-audio",
	"description": "Display GLL (Generic Loudspeaker Library) file data with interactive visualizations.",
	"keywords": [
		"gll",
		"loudspeaker",
		"audio",
		"acoustic",
		"frequency response"
	],
	"attributes": {
		"fileId": {
			"type": "number",
			"default": 0
		},
		"fileUrl": {
			"type": "string",
			"default": ""
		},
		"fileName": {
			"type": "string",
			"default": ""
		},
		"showOverview": {
			"type": "boolean",
			"default": true
		},
		"showSources": {
			"type": "boolean",
			"default": true
		},
		"sourcesDisplayMode": {
			"type": "string",
			"default": "expandable",
			"enum": [ "compact", "detailed", "expandable" ]
		},
		"showSourceResponseCharts": {
			"type": "boolean",
			"default": false
		},
		"showResponses": {
			"type": "boolean",
			"default": true
		}
	},
	"example": {
		"attributes": {
			"fileName": "example-speaker.gll"
		}
	},
	"supports": {
		"html": false,
		"align": [ "wide", "full" ]
	},
	"textdomain": "gll-info",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
